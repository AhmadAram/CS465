
.data

array:
	.space 36# allocate enough space for 9 numbers

.text 

array_init1:
	li $t0, 0	#start our counter at 0
	la $a1, array#load the starting adress of the array into $a1
array_init2:
	sw $t0, 0($a1)	#store the counter/number at the adress in the array
	#li $v0, 1
	#lw $a0, ($a1)
	#syscall
	addi $a1, $a1, 4
	addi $t0, $t0, 1
	bne $t0, 10, array_init2
	la $a1, array
	addi $a1, $a1, 8
start:	#begin recursive function to generate the phone numbers
	li $t1, 0	#reset t0 to 0 
	addi $sp, $sp, -12	# make room on the stack for the variables
	sw $ra, 0($sp)	#store the return adress as we come back up from 
	sw $s0, 4($sp)	#store the value of $s0 onto the stack as a precatuion 
	sw $s1, 8($sp) 	#store the value of $s1 onto the stack as a precaution
	
	#la $a2, 36(array) <- put into a register
	
	beq $a1, $a2, end	#jump to the end once recursion is finished
	jal start	#jump back up to the start
end:
	
