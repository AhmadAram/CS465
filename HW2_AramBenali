
.data

array:
	.space 36

.text 

array_init1:
	li $t0, 0
	la $a1, array
array_init2:
	sw $t0, 0($a1)
	#li $v0, 1
	#lw $a0, ($a1)
	#syscall
	addi $a1, $a1, 4
	addi $t0, $t0, 1
	bne $t0, 10, array_init2
	la $a1, array
	addi $a1, $a1, 8
start:
	li $t1, 0
	addi $sp, $sp, -12
	sw $ra, 0($sp)
	sw $s0, 4($sp)
	sw $s1, 8($sp)
	
	#la $a2, 36(array) <- put into a register
	
	beq $a1, $a2, end
	jal start
end:
	